package basic_code_tip;

import java.util.Scanner;

public class Ex02 {
//	백준 2752번 : 세 수 정렬	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		int[] arr = {a,b,c};
		
		for(int i=0; i<1; i++) {
			for(int j=0; j<2; j++) {
				int tmp = 0;
				if(arr[j] > arr[j+1]) {
					arr[j] = tmp;
					arr[j] = arr[j+1];
					arr[j+1] = tmp;
				}
			}
		}
	}
}
