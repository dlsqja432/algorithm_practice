package datastructureex;

import java.util.Scanner;

public class AlgoExam {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.println(func1(n));
	}
	
	public static int func1(int n) {
		int rs = 0;
		for(int i=1; i<=n; i++) {
			if(i%3==0 || i%5==0) {
				rs += i;
			}
		}
		return rs;
	}
}

