package array;

import java.util.Scanner;

// 백준 10807번 : 개수 세기
public class Ex05 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] arr = new int[201];
		for(int i=0; i<n; i++) {
			int a = sc.nextInt();
			arr[a]++;
		}
		int v = sc.nextInt();
		System.out.println(arr[v]);
	}
}
