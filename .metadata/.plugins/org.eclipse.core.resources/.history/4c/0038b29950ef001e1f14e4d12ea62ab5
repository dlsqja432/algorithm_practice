package datastructureex;

import java.util.Scanner;

public class AlgoExam {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] arr = new int[n];
		for(int i=0; i<n; i++) {
			arr[i] = sc.nextInt();
		}
		
		System.out.println(func2(arr, n));
	}
	
	public static int func1(int n) {
		int rs = 0;
		for(int i=1; i<=n; i++) {
			if(i%3==0 || i%5==0) {
				rs += i;
			}
		}
		return rs;
	}
	
	public static int func2(int arr[],int n) {
		for(int i=0; i<n-1; i++) {
			for(int j=i+1; j<n; j++) {
				if(arr[i] + arr[j] == 100) {
					return 1;
				}
			}
		}
		return 0;
	}
}

