package recursion;

import java.util.Scanner;

//백준2447번: 별 찍기 - 10
public class Ex08 {
	static int n;
	static String[][] board;
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		sc.close();
		board = new String[n][n];
		
		for(int i=0; i<n; i++) {
			for(int j=0; j<n; j++) {
				board[i][j] = "*";
			}
		}
		
		printStar(0,0,n);
	}
	
	static void printStar(int x, int y,int size) {
		
	}
}
