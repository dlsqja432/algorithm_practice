package array;

import java.util.Scanner;

public class Ex03 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = sc.next();
		int[] arr = new int[10];
		
		for(int i=0; i<s.length(); i++) {
			arr[s.charAt(i)-'0']++;
		}
		
		int a = 0;
		if((arr[6]+arr[9])%2==1) {
			a = (arr[6]+arr[9])/2+1;
		} else a = (arr[6]+arr[9])/2;
		
		int b = arr[0]+arr[1]+arr[2]+arr[3]+arr[4]+arr[5]+arr[7]+arr[8]; 
		
		System.out.println();
	}
}
