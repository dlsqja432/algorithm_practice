package datastructureex;

import java.util.Scanner;

public class AlgoExam {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
	}
	
	public static int func1(int n) {
		int cnt = 0;
		for(int i=1; i<=n; i++) {
			if(i%3==0 || i%5==0) {
				cnt += i;
			}
		}
		return cnt;
	}
}
